  "name": "dsfrontend",
  "version": "2.0.0",
  "private": true,
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dataquestio/dsfrontend.git"
  },
  "engines": {
    "node": ">=4.2.0",
    "npm": "^3.0.0"
  },
  "scripts": {
    "postinstall": "cd node_modules/grunt-contrib-imagemin && rm -rf node_modules/vinyl-fs && npm i vinyl-fs@2.2.x && npm install && cd - && cd node_modules/enzyme && npm i && npm run prepublish && cd -",
    "update:enzyme": "cd node_modules/enzyme && npm i && npm run prepublish && cd -",
    "clean": "rimraf dist/react",
    "compile": "better-npm-run compile",
    "lint": "npm run lint:new",
    "lint:fix": "npm run lint -- --fix",
    "lint:all": "eslint .",
    "lint:new": "eslint ./src",
    "start": "better-npm-run start",
    "dev": "better-npm-run dev",
    "dev:nw": "npm run dev -- --nw",
    "dev:no-debug": "npm run dev -- --no_debug",
    "test": "better-npm-run test",
    "test:dev": "npm run test -- --watch & npm run view-coverage",
    "view-coverage": "serveur -R -L -b -o coverage/html -p 3001",
    "deploy": "better-npm-run deploy",
    "flow:check": "babel-node bin/flow-check",
    "codecov": "cat coverage/*/lcov.info | codecov"
  },
  "betterScripts": {
    "compile": {
      "command": "npm run clean && babel-node bin/compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "dev": {
      "command": "nodemon --exec babel-node bin/server",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "deploy": {
      "command": "grunt build && npm run test && npm run compile",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "start": {
      "command": "babel-node bin/server",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "test": {
      "command": "babel-node ./node_modules/karma/bin/karma start build/karma.conf",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": "app:*"
      }
    }
  },
